{
  "name": "leantable",
  "version": "0.2.0",
  "description": "A fast, extensible table component",
  "keywords": [
    "table",
    "grid",
    "react",
    "component",
    "solid",
    "solidjs"
  ],
  "author": "erictooth",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/erictooth/leantable.git"
  },
  "homepage": "https://github.com/erictooth/leantable",
  "bugs": {
    "url": "https://github.com/erictooth/leantable/issues"
  },
  "files": [
    "dist-cjs",
    "dist-esm"
  ],
  "exports": {
    "./core": {
      "import": "./dist-esm/core/index.js",
      "require": "./dist-cjs/core/index.js"
    },
    "./plugins": {
      "import": "./dist-esm/plugins/index.js",
      "require": "./dist-cjs/plugins/index.js"
    },
    "./utils": {
      "import": "./dist-esm/utils/index.js",
      "require": "./dist-cjs/utils/index.js"
    }
  },
  "packageManager": "pnpm@7.1.6",
  "devDependencies": {
    "@erictooth/eslint-config": "^8",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.194",
    "@types/react": "^18.0.9",
    "chokidar-cli": "^3",
    "eslint": "^8",
    "glob": "^8",
    "prettier": "~2.6",
    "react": "17",
    "react-dom": "17",
    "typescript": "~4.7"
  },
  "scripts": {
    "prepack": "make prepack"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "solid-js": "^1.4.3"
  }
}
